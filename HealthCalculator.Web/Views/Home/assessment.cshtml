    function Uploadocument(folderDetails) {
        // var folderDetails = "";
        // folderDetails = "1";

        var fileUpload = null;
        //var fileUpload = $("#imageBrowes").get(0);
        if (folderDetails == 1) {
            fileUpload = $("#imageBrowes").get(0);
        }
        else {
            fileUpload = $("#imageBrowesAfter").get(0);
        }

        var files = fileUpload.files;
        if (files.length > 0) {

            // Create FormData object
            var fileData = new FormData();
            // Looping over all files and add it to FormData object
            for (var i = 0; i < files.length; i++) {
                files[i].name = files[i].name + folderDetails;
                fileData.append(files[i].name, files[i]);
            }

            //We will change the below logic later error handling
            $.ajax({
                url: virtualPath + '/Program/UploadDocument?userID=' + folderDetails,
                type: "POST",
                contentType: false, // Not to set any content header
                processData: false, // Not to process data
                data: fileData,//{ 'fileData': fileData, 'id': submissionid},
                beforeSend: function (request) {
                    //request.setRequestHeader('ID', sessionStorage.getItem('ID'));
                    // request.setRequestHeader('ID', sessionStorage.getItem('Id'));
                },
                success: function (result) {

                    if (result.ErrorMessage == undefined) {
                        if (folderDetails == 1) {
                            $("#hdnProfilePhotoPath").val(result);
                        }
                        else {
                            $("#hdnProfilePhotoPathAfter").val(result);
                        }


                    }
                    else {
                        alert(result.ErrorMessage);
                    }
                }

            });
        }
    }


