    <script type="text/javascript">
    var isValidAdult = true;

    var isValidChild = true;

    function validateChild() {
        //
        isValidChild = true;
        if ($("#txtChildEmail").val() == "" && isValidChild == true) {
            alert("Please enter email address");
            isValidChild = false;
            $("#txtChildEmail").focus();
            return false;
        }
        //if ($("#txtChildEmail").val() != "") {
        //    if (!ValidateEmail($("#txtChildEmail").val()),"Please enter correct mail id.") {
        //        alert("Please enter email address");
        //        return false;
        //    }
        //}
        if ($("#txtChildContactNo").val() == "" && isValidChild == true) {
            alert("Please enter contact number.");
            $("#contactNo").focus();
            return false;
        }
        if ($("#txtChildParentGurdian").val() == "" && isValidChild == true) {
            alert("Please enter Parent Gurdian name.");
            $("#txtChildParentGurdian").focus();
            return false;
        }
        if ($("#txtChildLoacation").val() == "" && isValidChild == true) {
            alert("Please enter location.");
            return false;
        }
        return true;
    }

    function checkChildStepOne() {
        isValidChild = true;
        //
        if ($("#txtChildName").val() == "") {
            alert("Please enter child Name.");
            $("#txtChildName").focus();
            isValidChild = false;
        }
        if ($("#txtChildAge").val() == "Age"  && isValidChild == true) {
            alert("Please enter child Age.");
            isValidChild = false;
            $("#txtChildAge").focus();
        }
        if ($("#txtChildAge").val() != "" && isValidChild == true) {
            var chAge = parseInt($("#txtChildAge").val());
            if (chAge > 18)
            {
                alert("Child Age should be less than 18.");
                isValidChild = false;
                $("#txtChildAge").focus();
            }
        }
        if ($("#ddlChildGender").val() == "Gender" && isValidChild == true) {
            alert("Please select gender.");
            isValidChild = false;
            $("#ddlChildGender").focus();
        }
        if ($("#ChildheightSelect").val() == "1" && isValidChild == true) {
            if (($("#ddlChildFeet").val() == "Feet" || $("#ddlChildInches").val() == "Inches") && isValidChild == true) {
                alert("Please select Height in feet & Inches.");
                $("#ddlChildFeet").focus();
                isValidChild = false;
            }

        } else {
            if ($("#ddlChildCentimeter").val() == "Centimeter" && isValidChild == true) {
                alert("Please enter Height in Centimeter");
                $("#ddlChildCentimeter").focus();
                isValidChild = false;
            }
        }
        if ($("#ChildWeightSelect").val() == "1" && isValidChild == true) {
            if (($("#ddlChildKilo").val() == "Kilo" || $("#ddlChildGaram").val() == "Grams") && isValidChild == true) {
                alert("Please enter the weight in Kilo and gram");
                $("#ddlChildKilo").focus();
                isValidChild = false;
            }
        }
        else {
            if ($("#ddlChildPounds").val() == "Pounds" && isValidChild == true) {
                alert("Please enter weight in Pounds");
                $("#ddlChildPounds").focus();
                isValidChild = false;
            }
        }

    }

    function checkAdultStepTwo() {
        //
        isValidAdult = true;
        if (($("#chkAOthers").prop("checked") == true && $("#otherHealthProblem").val() == "")) {
            alert("Please enter health issues other in below textbox.");
            $("#otherHealthProblem").focus();
            isValidAdult = false;
            return false;
        }
    }

    function checkAdultStepFour() {
        //
